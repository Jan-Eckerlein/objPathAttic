{
  "name": "obj-path-attic",
  "version": "0.0.1",
  "description": "Utility package for handling deeply nested object paths.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "lint": "npx eslint src/**",
    "format": "npx prettier . --write",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "path",
    "obj",
    "object",
    "nested",
    "prop",
    "dot notation",
    "dot",
    "object-path",
    "nested-data",
    "utility",
    "javascript",
    "typescript"
  ],
  "author": {
    "name": "Jan Eckerlein",
    "email": "eckerlein.jan@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jan-Eckerlein/objPathAttic"
  },
  "bugs": {
    "url": "https://github.com/Jan-Eckerlein/objPathAttic"
  },
  "homepage": "https://github.com/Jan-Eckerlein/objPathAttic#readme",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "prettier": "3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
